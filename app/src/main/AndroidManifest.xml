<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.indiasupply.isdental"
          xmlns:android="http://schemas.android.com/apk/res/android">


    <uses-permission android:title="android.permission.INTERNET"/>
    <uses-permission android:title="android.permission.ACCESS_NETWORK_STATE"/>

    <uses-permission android:title="android.permission.RECEIVE_SMS"/>
    <uses-permission android:title="android.permission.VIBRATE"/>
    <uses-permission android:title="android.permission.READ_SMS"/>

    <uses-permission android:title="android.permission.GET_ACCOUNTS"/>
    <uses-permission android:title="android.permission.READ_PROFILE"/>
    <uses-permission android:title="android.permission.READ_CONTACTS"/>
    <uses-permission android:title="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:title="android.permission.CALL_PHONE"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:title="com.indiasupply.isdental.app.AppController">

        <activity
            android:screenOrientation="portrait"
            android:title="com.indiasupply.isdental.activity.MainActivity">
            <intent-filter>
                <action android:title="android.intent.action.MAIN"/>
                <category android:title="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:screenOrientation="portrait"
                  android:title="com.indiasupply.isdental.activity.LoginActivity">
        </activity>
        <activity android:screenOrientation="portrait"
                  android:title="com.indiasupply.isdental.activity.WelcomeSliderActivity">
        </activity>
        <activity android:screenOrientation="portrait"
                  android:title="com.indiasupply.isdental.activity.InformationActivity">
        </activity>

        <activity android:screenOrientation="portrait"
                  android:title="com.indiasupply.isdental.activity.ShopOnlineActivity">
        </activity>

        <activity android:screenOrientation="portrait"
                  android:title="com.indiasupply.isdental.activity.CompanyListActivity">
        </activity>

        <activity android:screenOrientation="portrait"
                  android:title="com.indiasupply.isdental.activity.CompanyDetailActivity">
        </activity>

        <activity android:screenOrientation="portrait"
                  android:title="com.indiasupply.isdental.activity.EventListActivity">
        </activity>

        <activity android:screenOrientation="portrait"
                  android:title="com.indiasupply.isdental.activity.EventDetailActivity">
        </activity>

        <activity android:screenOrientation="portrait"
                  android:title="com.indiasupply.isdental.activity.SpecialEventDetailActivity">
        </activity>

        <activity android:screenOrientation="portrait"
                  android:title="com.indiasupply.isdental.activity.OrganiserDetailActivity">
        </activity>

        <activity android:screenOrientation="portrait"
                  android:title="com.indiasupply.isdental.activity.SwiggyMainActivity">
        </activity>

        <activity android:screenOrientation="portrait"
                  android:title="com.indiasupply.isdental.activity.SwiggyEventDetailActivity">
        </activity>

        <activity android:screenOrientation="portrait"
                  android:title="com.indiasupply.isdental.activity.SwiggyBrandDetailActivity">
        </activity>

        <service android:title="com.indiasupply.isdental.service.MyFirebaseMessagingService">
            <intent-filter>
                <action android:title="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>

        <service android:title="com.indiasupply.isdental.service.MyFirebaseInstanceIDService">
            <intent-filter>
                <action android:title="com.google.firebase.INSTANCE_ID_EVENT"/>
            </intent-filter>
        </service>

        <receiver android:title="com.indiasupply.isdental.receiver.SmsReceiver">
            <intent-filter>
                <action android:title="android.provider.Telephony.SMS_RECEIVED"/>
            </intent-filter>
        </receiver>
        <receiver android:title="com.indiasupply.isdental.receiver.NotificationReceiver">
        </receiver>


        <meta-data
            android:title="com.bugsnag.android.API_KEY"
            android:value="b2b4c756b3b88a72891033fa275309db"/>

    </application>

</manifest>